{
  "name": "sokos-mikrofrontend-template",
  "version": "1.0.0",
  "description": "Mikrofrontend template",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "prepare": "husky install",
    "lint": "eslint --fix --ext .ts,.tsx,.js,.jsx .",
    "pretty-quick": "pretty-quick --pattern \"**/*.*(ts|tsx)\""
  },
  "dependencies": {
    "@navikt/ds-css": "^3.1.2",
    "@navikt/ds-css-internal": "^3.1.2",
    "@navikt/ds-react": "^3.1.2",
    "@navikt/ds-react-internal": "^3.1.2",
    "@navikt/ds-tokens": "^3.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "swr": "^2.1.5"
  },
  "devDependencies": {
    "@eik/rollup-plugin": "^4.0.49",
    "@rollup/plugin-terser": "^0.4.1",
    "@types/react": "^18.2.6",
    "@types/react-dom": "^18.2.4",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@vitejs/plugin-react": "^4.0.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.2",
    "husky": "^8.0.3",
    "jsdom": "^22.0.0",
    "lint-staged": "^13.2.2",
    "postcss": "^8.4.23",
    "prettier": "^2.8.8",
    "pretty-quick": "^3.1.3",
    "tailwindcss": "^3.3.2",
    "vite": "^4.3.6",
    "vite-plugin-css-injected-by-js": "^3.1.1",
    "vite-plugin-mock": "^2.9.8",
    "vitest": "^0.31.0"
  },
  "eslintConfig": {
    "extends": [
      "plugin:jsx-a11y/recommended",
      "plugin:@typescript-eslint/recommended"
    ]
  },
  "prettier": {
    "printWidth": 120
  },
  "lint-staged": {
    "*.{ts,tsx,css,md,html,json}": [
      "prettier --cache --write"
    ]
  },
  "engines": {
    "node": ">=18.15.0"
  }
}
